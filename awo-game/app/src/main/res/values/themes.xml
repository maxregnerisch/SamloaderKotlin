        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/awo_red</item>
        <item name="colorPrimaryVariant">@color/awo_blue</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/awo_green</item>
        <item name="colorSecondaryVariant">@color/awo_orange</item>
